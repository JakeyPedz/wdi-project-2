<div class="row">
<h1>Anecists</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <div class="small-6 medium-6 large-3 columns user-tile">
        <ul>
        <li class="user-tile-image"><%= link_to (image_tag user.image.thumb.url), user %></li>
        <li class="strong"><%= link_to user.username, user %></li>
        <li><%= link_to user.tagline, user %></li>
      </ul>
      </div>
    </li>
  <% end %>
</ul>
</div>